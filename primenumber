num=int(input("enter a number ="))



a=0
if num==1:
     print("number is not a prime")
else:
        for i in range (2,num):
          if (num % i)==0:
             print("number is a not a prime")
             a=1
             break
             

if a==0 and num!=1:
    print("number is a prime")    #"number is a prime"
